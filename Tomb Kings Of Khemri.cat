        <profile id="8415-3e9d-b655-2412" name="The Blessed Blade Of Ptra" publicationId="0db1-ce28-da27-1d5b" page="37" hidden="false" typeId="576561706f6e23232344415441232323" typeName="Weapon">
          <characteristics>
            <characteristic name="Range" typeId="52616e676523232344415441232323">Combat</characteristic>
